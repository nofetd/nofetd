all: encoder
encoder: task2b.o util.o start.o
	ld -m elf_i386 start.o task2b.o util.o -o encoder
task2b.o: flame2.c
	gcc -m32 -Wall -ansi -c -nostdlib -fno-stack-protector  task2b.c -o task2b.o
util.o: util.c
	gcc -m32 -Wall -ansi -c -nostdlib -fno-stack-protector  util.c -o util.o

start.o: start.s
	nasm -f elf32 start.s -o start.o
.PHONY: clean
clean: 
	rm -f *.o encoder
